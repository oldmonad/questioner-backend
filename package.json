{
  "name": "Questioner",
  "version": "1.0.0",
  "description": "a really cool app",
  "main": "index.js",
  "engines": {
    "node": "8.x",
    "npm": "6.x"
  },
  "scripts": {
    "start": "node ./build/server.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run build-babel",
    "heroku-postbuild": "npm run build",
    "lint": "eslint",
    "drop-tables": "babel-node ./server/db/migrationsDropTables.js",
    "create-tables": "babel-node  ./server/db/migrationsCreateTables.js",
    "migrate": "npm run drop-tables && npm run create-tables",
    "start:dev": "nodemon --exec babel-node ./server/server.js",
    "test": "npm run migrate && nyc --reporter=html --reporter=text mocha --compilers js:@babel/register --recursive ./server/test/**/*.spec.js --reporter spec --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbytecoderc/Questioner.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dbytecoderc/Questioner/issues"
  },
  "homepage": "https://github.com/dbytecoderc/Questioner#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "browser-sync": "^2.26.5",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "nyc": "^14.1.1",
    "rollup": "^1.1.2",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.3.1",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "express-validator": "^5.3.1",
    "http": "0.0.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "live-server": "^1.2.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "services": "0.0.3",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2",
    "validatorjs": "^3.15.1"
  }
}
